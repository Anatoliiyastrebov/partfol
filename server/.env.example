# SendGrid API Configuration
# Получите API ключ в SendGrid Dashboard: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=SG.your_sendgrid_api_key_here

# Email Configuration
# EMAIL_FROM должен быть verified sender в SendGrid Dashboard
# Проверьте в: https://app.sendgrid.com/settings/sender_auth/senders
EMAIL_FROM=your-email@example.com

# Email получателя (куда будут приходить письма с формы)
EMAIL_TO=your-email@example.com

# Server Configuration
PORT=3000

# Frontend URL (для CORS, опционально)
FRONTEND_URL=http://localhost:8000
